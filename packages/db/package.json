{
  "name": "@pokus3/db",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./models/*": {
      "types": "./dist/models/*.model.d.ts",
      "default": "./src/models/*.model.ts"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./src/*.ts"
    }
  },
  "scripts": {
    "build": "tsdown"
  },
  "devDependencies": {
    "@pokus3/config": "workspace:*",
    "@types/mongoose-delete": "^1.0.6",
    "tsdown": "catalog:"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@pokus3/queue": "workspace:*",
    "cron-parser": "^5.4.0",
    "dotenv": "catalog:",
    "mongoose": "^8.14.0",
    "mongoose-delete": "^1.0.6",
    "zod": "catalog:"
  }
}
